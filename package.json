{
  "name": "poseidon-ui",
  "version": "0.1.0",
  "description": "An UI to rules them all !",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/poseidon.js && NODE_ENV=production webpack src/index.js dist/poseidon.min.js",
    "lint": "eslint src test examples",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --recursive",
    "test:watch": "NODE_ENV=test mocha --compilers js:babel/register --recursive --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --recursive",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tychotatitscheff/poseidon-ui.git"
  },
  "keywords": [
    "library-boilerplate-keywords"
  ],
  "author": "Tycho Tatitscheff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tychotatitscheff/poseidon-ui/issues"
  },
  "homepage": "https://github.com/tychotatitscheff/poseidon-ui",
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.6.18",
    "babel-eslint": "^3.1.15",
    "babel-loader": "^5.3.2",
    "chai": "^3.0.0",
    "core-js": "^0.9.18",
    "eslint": "^0.23",
    "eslint-config-airbnb": "0.0.6",
    "eslint-plugin-react": "^2.3.0",
    "expect": "^1.6.0",
    "isparta": "^3.0.3",
    "karma": "^0.12.31",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.6.0",
    "mocha": "^2.2.5",
    "rimraf": "^2.3.4",
    "sinon": "^1.15.4",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "invariant": "^2.0.0",
    "modernizr": "^3.0.0-alpha.4",
    "radium": "^0.13.4",
    "react": "^0.14.0-beta1"
  }
}
